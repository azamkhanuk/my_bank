<h1>Sign Up and get 100 cat coins </h1>


<%= form_for @user do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %> <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %> <br>
  <%= f.label :email %>
  <%= f.text_field :email %> <br>
  <%= f.label :age %>
  <%= f.number_field :age %> <br>
  <%= f.label :gender %>
  <%= f.text_field :gender %> <br>
  <%= f.label :password %>
  <%= f.password_field :password %>


  <%= f.submit 'Sign Up'%>

<%end %>
